{
  "name": "flcss",
  "version": "2.2.4",
  "description": "A bad attempt at CSS-in-JS",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "watch": "npx tsc --lib es2015,dom -w -d src/*.ts --outDir build",
    "build": "rm -rf ./build && mkdir ./build && npx tsc --lib es2015,dom --removeComments -d src/*.ts --outDir build",
    "test": "jest",
    "test:coverage": "jest --collect-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ker0olos/flcss.git"
  },
  "keywords": [
    "css-in-js",
    "css"
  ],
  "author": "Kerolos Zaki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ker0olos/flcss/issues"
  },
  "homepage": "https://github.com/ker0olos/flcss#readme",
  "dependencies": {
    "construct-style-sheets-polyfill": "^2.4.2",
    "csstype": "^3.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.12.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3"
  }
}
